<simulation:SimulationLevelGroup
		xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:mx="library://ns.adobe.com/flex/mx"
		xmlns:simulation="collaboRhythm.plugins.bloodPressure.view.simulation.*"
		width="100%" height="100%">
	<simulation:showEffect>
		<s:Fade alphaFrom="0" alphaTo="1"/>
	</simulation:showEffect>
	<simulation:hideEffect>
		<s:Fade alphaFrom="1" alphaTo="0"/>
	</simulation:hideEffect>
	<simulation:HypertensionWholeBody id="hypertensionWholeBody"
									  percentWidth="100" percentHeight="100"
									  />
	<simulation:ContentButton
			x="{367.072 + 25.6045}" y="{126.56 + 1.5}"
			width="400" height="60"
			cornerRadius="5"
			chromeColor="#D1D3D4"
			fontFamily="Myriad Pro Light" fontSize="30" fontWeight="bold" lineHeight="120%" color="#79a873" kerning="on"
			label="Hypertension"
			click="dispatchEvent(new SimulationLevelEvent(SimulationLevelEvent.DRILL_DOWN_LEVEL, true));"
			>
		<simulation:leftContent>
			<simulation:ProblemsIcon left="15" verticalCenter="0"/>
		</simulation:leftContent>
		<simulation:rightContent>
			<simulation:GaugeBackground right="10" verticalCenter="0"/>
		</simulation:rightContent>
	</simulation:ContentButton>
	<fx:Script><![CDATA[


		]]></fx:Script>
</simulation:SimulationLevelGroup>
